a = int (input ('primeiro valor: '))
b = int(input('segundo valor: '))
c = int(input('segundo valor: '))

if a > b and a > c:
    print('O maior valor é: {}'.format(a))
elif b > a and b > c:
    print('O maior valor é: {}'.format(b))
else:
    print('O maior valor é: {}'.format(c))
print ('Final do programa')