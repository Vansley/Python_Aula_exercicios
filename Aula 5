a = int(input('digite a primeira nota: '))
b = int(input('digite a segunda nota: '))
c = int(input('digite a terceira nota: '))
media = (a + b + c ) / 3
media == 7
print ('media: {}' .format (media))
if a >=7 and b >=7 and c >= 7:
     print ('voce foi aprovado')
else:
     print('vocÃª foi reprovado')